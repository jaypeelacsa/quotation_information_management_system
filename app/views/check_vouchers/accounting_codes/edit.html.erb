<section class="content-header">
	<h1>Check Account
		<small>Edit</small>
	</h1>
</section>

<section class="content">
	<%= simple_form_for [@check_voucher, @accounting_code], defaults: { wrapper_html: { class: 'form-group' }, input_html: {class: 'form-control'} }, html: { multipart: true, novalidate: true } do |ac| %>
  <form role="form">
     <div class="field">
      <%= ac.input :account_charge %>
     </div>
     <div class="field">
      <%= ac.input :account_code %>
     </div>
    <%= ac.submit class: "btn btn-success" %>
  </form>
  <% end %>
</section>

