= form_tag send("billings_path"),method: "GET" do
	.ui.icon.input.fluid
		%input{:name => "start_date", :placeholder => "Billed From", class: "datepick"}
	%br
	.ui.icon.input.fluid
		%input{:name => "end_date", :placeholder => "Billed To", class: "datepick"}

	%br
	.pull-right
		.ui.buttons
			%button.ui.positive.button Search
			.or
			= link_to "Clear", billings_path, class: "ui button"
